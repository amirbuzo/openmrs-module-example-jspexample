<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.1//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.jspexample">

	<class name="RoomTemperature" table="room_temperature">

		<id name="roomTemperatureId" type="int" column="room_temperature_id">
			<generator class="native" />
		</id>

		<property name="time" type="java.util.Date" column="time" not-null="true"/>
		
		<property name="temp" type="java.lang.Integer" not-null="true" />
		
		<property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true" />		
	</class>
	
</hibernate-mapping>
